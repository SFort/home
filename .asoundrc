pcm.!default {
	type plug
#	slave.pcm "hw:Loopback,0,0"
	slave.pcm "duplex"
}
pcm.obs {
	ipc_key 8822
	type dsnoop
	slave.pcm "hw:Loopback,1,0"
}
pcm.dmixe {
	ipc_key 9921
	type dmix
	slave.pcm "crea"
}
pcm.dsnoope {
	ipc_key 9912
	type dsnoop
	slave.pcm "hw:3,0"
}
pcm.duplex {
	type asym
	playback.pcm "dmixe"
	capture.pcm "dsnoope"
}
pcm.crea {
	format S32_LE
	rate 96000
	type hw
	card 0
	device 0
}
ctl.!default {
	type hw
	card 0
}
