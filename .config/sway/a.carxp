<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE CARLA-PROJECT>
<CARLA-PROJECT VERSION='2.0'>
 <EngineSettings>
  <ForceStereo>false</ForceStereo>
  <PreferPluginBridges>false</PreferPluginBridges>
  <PreferUiBridges>true</PreferUiBridges>
  <UIsAlwaysOnTop>false</UIsAlwaysOnTop>
  <MaxParameters>200</MaxParameters>
  <UIBridgesTimeout>4000</UIBridgesTimeout>
 </EngineSettings>

 <Transport>
  <BeatsPerMinute>120</BeatsPerMinute>
 </Transport>

 <!-- Noise repellent -->
 <Plugin>
  <Info>
   <Type>LV2</Type>
   <Name>Noise repellent</Name>
   <URI>https://github.com/lucianodato/noise-repellent</URI>
  </Info>

  <Data>
   <Active>Yes</Active>
   <ControlChannel>1</ControlChannel>
   <Options>0x3</Options>

   <Parameter>
    <Index>0</Index>
    <Name>Reduction amount</Name>
    <Symbol>reduction</Symbol>
    <Value>48</Value>
   </Parameter>

   <Parameter>
    <Index>1</Index>
    <Name>Thresholds offset</Name>
    <Symbol>offset</Symbol>
    <Value>12</Value>
   </Parameter>

   <Parameter>
    <Index>2</Index>
    <Name>Release</Name>
    <Symbol>release</Symbol>
    <Value>0</Value>
   </Parameter>

   <Parameter>
    <Index>3</Index>
    <Name>Masking</Name>
    <Symbol>masking</Symbol>
    <Value>1</Value>
   </Parameter>

   <Parameter>
    <Index>4</Index>
    <Name>Protect Transients</Name>
    <Symbol>transient_protection</Symbol>
    <Value>1</Value>
   </Parameter>

   <Parameter>
    <Index>5</Index>
    <Name>Residual whitening</Name>
    <Symbol>whitening</Symbol>
    <Value>100</Value>
   </Parameter>

   <Parameter>
    <Index>6</Index>
    <Name>Learn noise profile</Name>
    <Symbol>noise_learn</Symbol>
    <Value>0</Value>
   </Parameter>

   <Parameter>
    <Index>7</Index>
    <Name>Adaptive noise learn</Name>
    <Symbol>adaptive_noise_learn</Symbol>
    <Value>1</Value>
   </Parameter>

   <Parameter>
    <Index>8</Index>
    <Name>Reset noise profile</Name>
    <Symbol>reset_noise_profile</Symbol>
    <Value>0</Value>
   </Parameter>

   <Parameter>
    <Index>9</Index>
    <Name>Residual listen</Name>
    <Symbol>Residual_listen</Symbol>
    <Value>0</Value>
   </Parameter>

   <CustomData>
    <Type>http://lv2plug.in/ns/ext/atom#Int</Type>
    <Key>https://github.com/lucianodato/noise-repellent#fftsize</Key>
    <Value>AAQAAA==</Value>
   </CustomData>

   <CustomData>
    <Type>http://lv2plug.in/ns/ext/atom#Float</Type>
    <Key>https://github.com/lucianodato/noise-repellent#nwindow</Key>
    <Value>AAAAAA==</Value>
   </CustomData>

   <CustomData>
    <Type>http://lv2plug.in/ns/ext/atom#Vector</Type>
    <Key>https://github.com/lucianodato/noise-repellent#FFTp2</Key>
    <Value>

    </Value>
   </CustomData>
  </Data>
 </Plugin>

 <ExternalPatchbay>
  <Connection>
   <Source>alsa_in:capture_1</Source>
   <Target>Carla:audio-in1</Target>
  </Connection>
  <Connection>
   <Source>alsa_in:capture_2</Source>
   <Target>Carla:audio-in2</Target>
  </Connection>
 </ExternalPatchbay>
</CARLA-PROJECT>
